{
  "name": "collab-text-editor",
  "version": "0.1.1",
  "description": "Monaco editor bindings for Yjs",
  "main": "./dist/y-monaco.js",
  "module": "./src/y-monaco.js",
  "sideEffects": false,
  "scripts": {
    "start": "npm run demo",
    "demo": "webpack-dev-server --open-page .",
    "dist": "npm run clean && rollup -c",
    "test": "rollup -c && node test/test.node.js",
    "lint": "standard",
    "watch": "rollup -wc",
    "debug": "concurrently 'live-server --port=3443 --entry-file=test.html' 'npm run watch'",
    "preversion": "npm run lint && npm run dist && npm run test",
    "clean": "rm -rf dist demo/dist"
  },
  "files": [
    "dist/*",
    "src/*"
  ],
  "repository": {
    "type": "git"
  },
  "keywords": [
    "Yjs",
    "Monaco"
  ],
  "license": "MIT",
  "dependencies": {
    "lib0": "^0.2.33"
  },
  "peerDependencies": {
    "yjs": "^13.3.1 && >=13.3.1",
    "y-protocols": ">=1.0.1",
    "monaco-editor": "^0.20.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^8.4.0",
    "@types/node": "^12.12.54",
    "concurrently": "^4.1.2",
    "css-loader": "^4.2.1",
    "jsdom": "^15.2.1",
    "live-server": "^1.2.1",
    "monaco-editor": "^0.17.1",
    "rollup": "^2.23.0",
    "rollup-plugin-postcss": "^3.1.4",
    "standard": "^14.3.4",
    "style-loader": "^1.2.1",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "y-protocols": "^1.0.1",
    "y-websocket": "^1.3.1",
    "yjs": "^13.3.1"
  }
}
